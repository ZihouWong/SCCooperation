<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name="struts.objectFactory" value="spring" />


<package name="lee" namespace="/" extends="struts-default">

<!-- action,spring结合，class已在spring里装配好 -->
<action name="login" class="loginAction">
<result type="chain" name="success">luntan</result>
<result name="error">/WEB-INF/page/sp.jsp</result>
</action>

<action name="insertUser" class="InsertUserAction">
<result name="success">/WEB-INF/page/imisindex.jsp</result>
</action>
<action name="pregister">
<result>/WEB-INF/page/webpages/register-p.html</result>
</action>
<action name="cregister">
<result>/WEB-INF/page/webpages/register-comp.html</result>
</action>
<action name="luntan" class="LuntanAction">
<result>/WEB-INF/page/webpages/luntan.jsp</result>
</action>
<action name="addNote" class="AddNoteAction">
<result type="chain" name="success">luntan</result>
</action>
<action name="minutenote" class="MinuteNoteAction">
<result>/WEB-INF/page/webpages/luntan-info.jsp</result>
</action>
<action name="addreplynote" class="AddReplyNoteAction">
<result type="chain" name="success">minutenote</result>
</action>
<action name="seex">
<result name="success">/WEB-INF/page/webpages/luntan-info.html</result>
</action>


<!-- 创兴谷 --> 
<action name="chuangxinggutest" class="ChuangxingguAction">
<result name="success">/WEB-INF/page/webpages/chuangxinggu/chuangxinggu.jsp</result>
</action>

<action name="showmorejob" class="ShowMoreJobAction">
<result name="success">/WEB-INF/page/webpages/chuangxinggu/showmorejob.jsp</result>
</action>

<action name="showjobinfo" class="ShowJobInfoAction">
<result name="success">/WEB-INF/page/webpages/chuangxinggu/showjobinfo.jsp</result>
</action>

<!-- 躬行课堂 -->
<action name="gongxingketangtest" class="GongxingketangAction">
<result name="success">/WEB-INF/page/webpages/gongxingketang/gongxingketang.jsp</result>
</action>

<action name="showcourseinfo" class="ShowCourseInfoAction">
<result name="success">/WEB-INF/page/webpages/gongxingketang/shoucourseinfo.jsp</result>
</action>

<!-- 黄智豪 END -->

<!-- test   ....................................................................... -->
<action name="chuangxinggu">
<result name="success">/WEB-INF/page/webpages/chuangxinggu/testchuangxinggu.jsp</result>
</action>
<action name="gongxing">
<result name="success">/WEB-INF/page/webpages/work/gongxing.html</result>
</action>
<action name="geren">
<result name="success" >/WEB-INF/page/webpages/people.html</result>
</action>

<action name="newcourseinfo">
<result name="success">/WEB-INF/page/webpages/gongxingketang/NewFile.jsp</result>
</action>


</package>


</struts>    
